<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="bqr_overhead_category_view_tree" model="ir.ui.view">
            <field name="name">bqr.overhead.category.view.tree</field>
            <field name="model">bqr.overhead</field>
            <field name="arch" type="xml">
                <tree editable="top">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="bqr_overhead_category_action" model="ir.actions.act_window">
            <field name="name">Overhead Category</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">bqr.overhead</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>