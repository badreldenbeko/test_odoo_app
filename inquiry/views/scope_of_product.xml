<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="scope_of_product_view_tree" model="ir.ui.view">
            <field name="name">scope_of_product_view_tree</field>
            <field name="model">scope.of.product</field>
            <field name="arch" type="xml">
                <tree string="scope_of_product_tree" editable="top">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="scope_of_product_action" model="ir.actions.act_window">
            <field name="name">Project Realization Plan Requirements</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">scope.of.product</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
</odoo>