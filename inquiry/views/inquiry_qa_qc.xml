<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="inquiry_qa_qc_view_tree" model="ir.ui.view">
            <field name="name">inquiry.qa.qc.view.tree</field>
            <field name="model">inquiry.qa.qc</field>
            <field name="arch" type="xml">
                <tree string="inquiry_qa_qc_tree">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="inquiry_qa_qc_action" model="ir.actions.act_window">
            <field name="name">QC/QA</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">inquiry.qa.qc</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>