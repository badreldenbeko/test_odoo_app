<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="inquiry_commercial_view_tree" model="ir.ui.view">
            <field name="name">inquiry_commercial_view_tree</field>
            <field name="model">inquiry.commercial</field>
            <field name="arch" type="xml">
                <tree string="inquiry_commercial_tree" editable="top">
                    <field name="name"/>
                </tree>
            </field>
        </record>

        <record id="inquiry_commercial_action" model="ir.actions.act_window">
            <field name="name">Commercial</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">inquiry.commercial</field>
            <field name="view_mode">tree,form</field>
        </record>

    </data>
</odoo>